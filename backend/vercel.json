{
    "version": 2,
    "builds": [
        {
            "src": "vercel_app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "vercel_app.py"
        },
        {
            "src": "/static/(.*)",
            "dest": "vercel_app.py"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}